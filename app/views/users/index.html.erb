<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td colspan="3"></td>
        <td><%= user.email %></td>
        <td colspan="4"></td>
        <td><%= link_to '  Profile', user %></td>
        <td colspan="3"></td>
      </tr>
    <% end %>
  </tbody>
</table>

